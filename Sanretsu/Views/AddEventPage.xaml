<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Sanretsu.Views.AddEventPage">
    <ContentPage.Content>
        <StackLayout x:Name="StackContainer">
            <Label Text="Click button to start scanning:" />
            <Button Text="Scan" Margin="10" Clicked="OnScanClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>